	HAL_Delay(50);

	uint8_t v_B = gConfig.ch_conf[channel_B].cr0 | CR0_1SHOT;
	max31856_wr_reg_mux_B(&tc[channel_B],MAX31856_CR0,&v_B,1);

	HAL_Delay(300);
	max31856_waitdrdy_mux(300);

