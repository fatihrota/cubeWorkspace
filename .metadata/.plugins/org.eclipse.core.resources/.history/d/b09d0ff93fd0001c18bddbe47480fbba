void tc_sel_ch(uint8_t ch)
{
	ch = ch % 4;

	switch(ch)
	{
	case 0:
		gTCMuxGPIOs[TC_MUX_SEL_A_GPIO].port->BSRR = gTCMuxGPIOs[TC_MUX_SEL_A_GPIO].pin;
